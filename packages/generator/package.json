{
  "name": "@runtyping/generator",
  "version": "9.0.1",
  "description": "The core generator package for runtyping",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "release": "semantic-release && sleep 3",
    "schema": "typescript-json-schema --out ../../runtyping.schema.json src/runtypes.ts Instructions && prettier --write ../../runtyping.schema.json",
    "start": "tsc --watch --preserveWatchOutput",
    "test": "jest test"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/runtyping/issues"
  },
  "homepage": "https://github.com/johngeorgewright/runtyping#readme",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.1.0",
    "@semantic-release/npm": "9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "29.5.14",
    "@types/js-yaml": "4.0.9",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.2",
    "@types/prettier": "2.7.3",
    "@types/yargs": "17.0.33",
    "jest": "29.7.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.10",
    "semantic-release": "20.1.3",
    "semantic-release-monorepo": "7.0.8",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.2.2",
    "typescript-json-schema": "0.62.0"
  },
  "dependencies": {
    "@johngw/array": "3.3.2",
    "@johngw/error": "2.2.2",
    "@johngw/iterator": "1.2.2",
    "@ts-morph/common": "0.21.0",
    "js-yaml": "4.1.0",
    "json-schema-to-typescript": "13.1.1",
    "lodash": "4.17.21",
    "ts-morph": "20.0.0",
    "tslib": "2.6.2",
    "yargs": "17.7.2",
    "zod": "3.22.4"
  },
  "peerDependencies": {
    "typescript": ">= 4.7.4"
  }
}
