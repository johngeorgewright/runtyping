{
  "name": "@runtyping/zod",
  "version": "5.0.1",
  "description": "Generate zod from static types & JSON schema.",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "runtyping": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "start": "tsc --watch --preserveWatchOutput",
    "test": "vitest run"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@runtyping/test-type-writers": "workspace:*",
    "@types/json-schema": "7.0.15",
    "@types/node": "24.10.8",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "vitest": "4.0.16",
    "zod": "4.3.5"
  },
  "dependencies": {
    "@runtyping/generator": "workspace:*",
    "@runtyping/zod-core": "workspace:*",
    "title-case": "4.3.2",
    "ts-morph": "27.0.2",
    "tslib": "2.8.1"
  }
}
