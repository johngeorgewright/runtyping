{
  "name": "@runtyping/test-type-writers",
  "version": "4.0.1",
  "description": "A testing library for runtyping typewriters",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./transformers/stringToNumber": {
      "types": "./dist/transformers/stringToNumber.d.ts",
      "default": "./dist/transformers/stringToNumber.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "start": "tsc --watch --preserveWatchOutput"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/runtyping/issues"
  },
  "homepage": "https://github.com/johngeorgewright/runtyping#readme",
  "peerDependencies": {
    "@runtyping/generator": "workspace:*",
    "@types/json-schema": "^7.0.11",
    "vitest": "^4.0.15"
  },
  "devDependencies": {
    "@runtyping/generator": "workspace:*",
    "@types/json-schema": "7.0.15",
    "@types/mkdirp": "2.0.0",
    "@types/node": "24.10.13",
    "rimraf": "6.1.3",
    "type-fest": "5.4.4",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "assert-never": "1.4.0",
    "mkdirp": "3.0.1",
    "ts-morph": "27.0.2",
    "tslib": "2.8.1"
  }
}
