{
  "name": "@runtyping/io-ts",
  "version": "4.0.2",
  "description": "Generate io-ts from static types & JSON schema.",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "runtyping": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "start": "tsc --watch --preserveWatchOutput",
    "test": "vitest run"
  },
  "peerDependencies": {
    "io-ts": "^2.0.0"
  },
  "devDependencies": {
    "@runtyping/test-type-writers": "workspace:*",
    "@types/json-schema": "7.0.15",
    "@types/node": "24.10.4",
    "fp-ts": "2.16.11",
    "io-ts": "2.2.22",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "dependencies": {
    "@johngw/array": "3.3.2",
    "@runtyping/generator": "workspace:*",
    "@ts-morph/common": "0.28.1",
    "title-case": "4.3.2",
    "ts-morph": "27.0.2",
    "tslib": "2.8.1"
  }
}
