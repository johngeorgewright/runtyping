    "@types/json-schema": "7.0.15",
    "@types/node": "24.10.3",
    "jest": "30.2.0",
    "rimraf": "6.1.2",
    "runtypes": "7.0.4",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "dependencies": {
